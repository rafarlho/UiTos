{
  "$schema": "node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "src/assets/api/swagger.json",
  "output": "src/app/api",
  "serviceSuffix": "Service",
  "enumNameSuffix": "",
  "modelSuffix": "",
  "module": true,
  "ignoreUnusedModels": true,
  "indexFile": true,
  "useOperationIdAsMethodName": true,
  "removePrefixFromModelNames": true,
  "generateExamples": false,
  "nullSafeModel": true,
  "sortParams": "path",
  "enumPropertyNaming": "camelCase",
  "modelPropertyNaming": "camelCase",
  "servicePropertyNaming": "camelCase"
}
